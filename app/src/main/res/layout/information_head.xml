<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ActionBar_Style">

    <ImageView
        android:id="@+id/back"
        android:clickable="true"
        android:onClick="click_to_back"
        style="@style/ActionBar_LeftImg_Style"
        android:src="@drawable/back"/>
    <TextView
        style="@style/ActionBar_CenterText_Style"
        android:text="@string/car_information"
        />
    <TextView
        style="@style/ActionBar_RightText_Style"
        android:text="@string/carinfo_actionbar_right"
        android:clickable="true"
        android:onClick="click_to_save_information"/>
</RelativeLayout>
